<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f3f1f1">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="17.14dp"
            android:layout_marginRight="17.14dp">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="29.14dp"
                android:fontFamily="@font/malgunfont"
                android:text="@string/register_form_titleText"
                android:textColor="#1e1e1e"
                android:textSize="13.71dp"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editText"
                android:layout_width="match_parent"
                android:layout_height="47.42dp"
                android:layout_marginTop="6dp"
                android:background="#ffffff"
                android:fontFamily="@font/malgunfont"
                android:hint="@string/register_form_emailHintText"
                android:paddingLeft="10.50dp"
                android:textColorHint="#afafaf"
                android:textSize="13.71dp"
                app:layout_constraintTop_toBottomOf="@+id/textView" />

            <EditText
                android:id="@+id/editText1"
                android:layout_width="match_parent"
                android:layout_height="47.42dp"
                android:layout_marginTop="3.71dp"
                android:background="#ffffff"
                android:hint="@string/register_form_PWHintText"
                android:paddingLeft="10.50dp"
                android:textColorHint="#afafaf"
                android:textSize="13.71dp"
                app:layout_constraintTop_toBottomOf="@+id/editText" />

            <EditText
                android:id="@+id/editText2"
                android:layout_width="match_parent"
                android:layout_height="47.42dp"
                android:layout_marginTop="3.71dp"
                android:background="#ffffff"
                android:fontFamily="@font/malgunfont"
                android:hint="@string/register_form_PWCheckHintText"
                android:paddingLeft="10.50dp"
                android:textColorHint="#afafaf"
                android:textSize="13.71dp"
                app:layout_constraintTop_toBottomOf="@+id/editText1" />

            <EditText
                android:id="@+id/editText3"
                android:layout_width="match_parent"
                android:layout_height="47.42dp"
                android:layout_marginTop="3.71dp"
                android:background="#ffffff"
                android:fontFamily="@font/malgunfont"
                android:hint="@string/register_form_nameHintText"
                android:paddingLeft="10.50dp"
                android:textColorHint="#afafaf"
                android:textSize="13.71dp"
                app:layout_constraintTop_toBottomOf="@+id/editText2" />

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="match_parent"
                android:layout_height="47.42dp"
                android:layout_marginTop="3.71dp"
                android:orientation="horizontal"
                app:layout_constraintTop_toBottomOf="@+id/editText3">


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <EditText
                        android:id="@+id/editText4"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:background="#ffffff"
                        android:fontFamily="@font/malgunfont"
                        android:hint="@string/register_form_RRHintText"
                        android:paddingLeft="10.50dp"
                        android:textColorHint="#afafaf"
                        android:textSize="13.71dp" />

                    <View
                        android:id="@+id/view"
                        android:layout_width="7.25dp"
                        android:layout_height="3dp"
                        android:layout_gravity="center"
                        android:background="#cdcdcd" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <EditText
                        android:id="@+id/editText5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="2"
                        android:background="#ffffff"
                        android:fontFamily="@font/malgunfont"
                        android:gravity="center"
                        android:hint="@string/register_form_RRbackHintText"
                        android:textColorHint="#afafaf"
                        android:textSize="13.71dp"
                        app:layout_constraintLeft_toLeftOf="@+id/view"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/editText3" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="8dp"
                        android:layout_weight="8"
                        android:fontFamily="@font/malgunfont"
                        android:text="@string/register_form_RRHideText"
                        android:textColor="#605f5f"
                        android:textSize="25.14dp"
                        app:layout_constraintBottom_toBottomOf="@+id/editText5"
                        app:layout_constraintLeft_toRightOf="@+id/editText5"
                        app:layout_constraintTop_toTopOf="@+id/editText5" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="match_parent"
                android:layout_height="47.42dp"
                android:layout_marginTop="48dp"
                android:orientation="horizontal"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout">

                <EditText
                    android:id="@+id/editText6"
                    android:layout_width="0dp"
                    android:layout_height="47.42dp"
                    android:layout_weight="7"
                    android:background="#ffffff"
                    android:fontFamily="@font/malgunfont"
                    android:hint="@string/register_form_phoneHintText"
                    android:paddingLeft="10.50dp"
                    android:textColorHint="#afafaf"
                    android:textSize="13.71dp" />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="6dp"
                    android:layout_weight="3"
                    android:scaleType="fitXY"
                    android:src="@drawable/find_id_request_btn" />
            </LinearLayout>

            <EditText
                android:id="@+id/editText7"
                android:layout_width="match_parent"
                android:layout_height="47.42dp"
                android:layout_marginTop="3.71dp"
                android:background="#ffffff"
                android:fontFamily="@font/malgunfont"
                android:hint="@string/register_form_CNHintText"
                android:paddingLeft="10.50dp"
                android:textColorHint="#afafaf"
                android:textSize="13.71dp"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout1" />


            <ImageView
                android:id="@+id/btn_next"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="84.75dp"
                android:layout_marginBottom="15dp"
                android:scaleType="fitXY"
                android:src="@drawable/next_btn"
                app:layout_constraintBottom_toBottomOf="parent" />

        </android.support.constraint.ConstraintLayout>
    </android.support.constraint.ConstraintLayout>
</layout>